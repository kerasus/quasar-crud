{
  "name": "quasar-crud",
  "version": "0.2.87",
  "description": "quasar crud. Uses Quasar Framework and Vue.",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "@quasar/extras": "^1.15.9",
    "axios": "^1.2.2",
    "core-js": "^3.27.1",
    "moment-jalaali": "^0.9.6",
    "quasar": "^2.11.3",
    "quasar-form-builder": "^0.1.70",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/compiler-sfc": "^3.2.45",
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^7.20.0",
    "postcss": "^8.4.20",
    "postcss-rtl": "^2.0.0",
    "sass": "1.52.3",
    "sass-loader": "^13.0.0",
    "vue-cli-plugin-quasar": "~5.0.1",
    "vue-router": "4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/attributes-order": "error",
      "vue/order-in-components": "error",
      "vue/html-indent": "error",
      "vue/this-in-template": "error",
      "vue/arrow-spacing": "error",
      "vue/block-spacing": "error",
      "vue/comma-dangle": "error",
      "vue/eqeqeq": "warn",
      "vue/multi-word-component-names": "off",
      "vue/key-spacing": "error",
      "vue/keyword-spacing": "error",
      "vue/no-deprecated-scope-attribute": "warn",
      "vue/no-deprecated-slot-attribute": "warn",
      "vue/no-deprecated-slot-scope-attribute": "warn",
      "vue/no-irregular-whitespace": "error",
      "vue/no-reserved-component-names": "error",
      "vue/valid-v-slot": "error",
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "contributors": [
    {
      "name": "Ali Esmaeeli",
      "email": "aliesmaeeli20@gmail.com",
      "url": "https://github.com/kerasus/quasar-crud"
    },
    {
      "name": "Maryam Razmjoo",
      "email": "MaryamRazmjoo76@yahoo.com",
      "url": "https://github.com/MaryamRazmjoo/quasar-crud"
    }
  ],
  "homepage": "https://github.com/kerasus/quasar-crud",
  "keywords": [
    "vue",
    "quasar",
    "quasar-crud"
  ]
}
